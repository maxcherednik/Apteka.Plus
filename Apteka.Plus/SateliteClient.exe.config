<configuration>
  <system.runtime.remoting>
    <application>
      <channels>
        <channel ref="tcp">
          <clientProviders>

            <formatter ref="binary" />

            <provider type="CustomSinks.CustomClientSinkProvider, CustomSinks"
							customSinkType="CompressionSink.CompressionClientSink, SampleSinks" />

          </clientProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>

</configuration>